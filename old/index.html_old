    <script type="text/javascript">
        $(document).ready(function() {
            $('form#htmlformwizard1').submit(function(e) {
                e.preventDefault();
                
                var x1 = document.forms["htmlformwizard1"]["name"].value;
                if (x1==null || x1=="") {
                    alert("Please enter your name.");
                    return false;
                }

                var x2 = document.forms["htmlformwizard1"]["email"].value;
                var atpos=x2.indexOf("@");
                var dotpos=x2.lastIndexOf(".");
                if (atpos<1 || dotpos<atpos+2 || dotpos+2>=x2.length) {
                    alert("Not a valid e-mail address.");
                    return false;
                }

                var x3 = document.forms["htmlformwizard1"]["phone"].value;                          
                if (x3==null || x3=="") {
                    alert("Please enter your phone number.");
                    return false;
                }

                var x4 = document.forms["htmlformwizard1"]["message"].value;                            
                if (x4==null || x4=="") {
                    alert("Message must be filled out.");
                    return false;
                }

                var x5 = document.forms["htmlformwizard1"]["captcha-txt"].value;                            
                if (x5==null || x5=="") {
                    alert("Captcha must be filled out.");
                    return false;
                }

                $.ajax({
                    url: 'verifycaptcha.php',
                    type: 'POST',
                    dataType: 'json',
                    data: {name: x1, email: x2, phone: x3, message: x4, captcha: x5, "_csrf-frontend": "PspL-Tb_Ya2fDAPscaTooGus3d-GKUIc1Z-2XmSExZV5mA-fWLIUmd1WSoUDib7TIvu6tO1wOEOd9foPCtaw7Q=="},
                    success: function(data) {
                        if (data.status == 1) {
                            location.reload();
                        } else {
                            alert("Captcha didn't match");
                            return false;
                        }
                    }
                });

                return false;
            });
        });
    </script>
